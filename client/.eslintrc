{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "plugins": ["react"],
  "env": {
    "browser": true
  },
  "rules": {
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "semi": ["error", "never"],
    "no-tabs": "off",
    "consistent-return": "off",
    "comma-dangle": ["error", "never"],
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
